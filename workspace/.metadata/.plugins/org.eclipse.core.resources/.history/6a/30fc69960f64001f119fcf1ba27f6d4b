	<script>
		$(function(){
			$('#btn1').on('click', function(){
                fetch('http://localhost/jqpro/%EB%B9%84%EB%8F%99%EA%B8%B05_fetch.html/memberController.jsp 404 (Not Found)')
                .then(function(res){
                    console.log(res);
                    if(res.ok){
                    	return res.json();
                    }else{
                    	new Error("Error: "+res.statusText);
                    }
                })
                .then(function(data){
                	console.log(data)
                })
                .catch(function(err){
                	alert(err);
                })
            })
		})
	</script>

